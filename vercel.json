{
  "version": 2,
  "env": {
    "DATABASE_HOST": "@POSTGRES_URL",
    "ACCESS_TOKEN_SECRET": "@ACCESS_TOKEN_SECRET"
  },
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PATCH", "DELETE"]
    }
  ]
}
